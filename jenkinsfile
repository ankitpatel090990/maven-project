pipeline{
	agent any
			stages{
				stage('SCM checkout'){
						steps{
						git branch: 'master', url: 'https://github.com/ankitpatel090990/maven-project.git'
						}
				}

				stage('validate code'){
						steps{
							withMaven(jdk: 'LOCAL_JDK', maven: 'LOCAL_MAVEN') {
									    sh 'mvn validate'
									}
						}
				}

				stage('complie'){
						steps{
							withMaven(jdk: 'LOCAL_JDK', maven: 'LOCAL_MAVEN') {
									    sh 'mvn complie'
									}
						}
				}

				stage('test'){
						steps{
							withMaven(jdk: 'LOCAL_JDK', maven: 'LOCAL_MAVEN') {
									    sh 'mvn test'
									}
						}
				}

				stage('package'){
						steps{
							withMaven(jdk: 'LOCAL_JDK', maven: 'LOCAL_MAVEN') {
									    sh 'mvn package'
									}
						}
				}



			}
}
